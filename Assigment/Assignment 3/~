#include<iostream>
#include<iomanip>
using namespace std;

int Pelajar[5][3];

int main()
{
	
	
}

void bacaData()
{
	cout << "Please enter the quiz data (separeted by spaces) for each student :" << endl;
	
	for(int i=0; i<5; i++){
		cout << "Student " << i " :" << endl;
		for(int j=0; j<3; j++){
			cin >> Pelajar[i][j] >> Pelajar[i][j] >> Pelajar[i][j];
		}
	}
	
	cout << endl << endl << "Output : " << endl;	
	
	
	for(int i=0; i<5; i++){
		for(int j=0; j<3; j++){
			cout << Pelajar[i][j] << Pelajar[i][j] << Pelajar[i][j];
		}
	}
}

double kiraMarkah()
{
	int studentMarks, totalMarks = 0;
	for(int i=0; i<5; i++){
		cout << "Markah Student " << i " :";
		for(int j=0; j<3; j++){
			studentMarks = Pelajar[i][j] + Pelajar[i][j] + Pelajar[i][j];
			totalMarks += studentMarks;
			
			cout << studentMarks;
		}
	}
	return totalMarks
}
